{
  "name": "pokemon-sv-151",
  "version": "1.0.0",
  "description": "pokemon-sv-151",
  "main": "code.js",
  "scripts": {
    "build:main": "esbuild plugin-src/code.ts --bundle --outfile=dist/code.js --target=es2017",
    "watch:main": "concurrently \"esbuild plugin-src/code.ts --bundle --outfile=dist/code.js --target=es2017 --watch\" \"tsc --noEmit -p plugin-src --watch\"",
    "build:ui": "npx vite build --minify esbuild --emptyOutDir=false",
    "watch:ui": "vite build --watch",
    "dev": "concurrently -n main,ui \"npm run watch:main\" \"npm run watch:ui\"",
    "test": "npm run build:main && npm run build:ui"
  },
  "author": "Figma",
  "license": "MIT License",
  "dependencies": {
    "@react-spring/three": "^10.0.3",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.3.0",
    "@react-three/postprocessing": "^3.0.4",
    "@types/three": "^0.179.0",
    "chroma-js": "^3.1.2",
    "maath": "^0.10.8",
    "postprocessing": "^6.37.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.4",
    "three": "^0.179.1"
  },
  "devDependencies": {
    "@figma/plugin-typings": "*",
    "@types/chroma-js": "^3.1.1",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "concurrently": "^8.2.2",
    "esbuild": "^0.25.11",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vite-plugin-glsl": "^1.5.4",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-svgr": "^4.5.0",
    "vite-svg-loader": "^5.1.0"
  }
}
